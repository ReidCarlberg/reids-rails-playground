<!DOCTYPE html>
<html>
<head>
  <title>ForceOsViz1</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<ul>
	<% if user_signed_in? %>
	  <li>
	  Logged in as <%= current_user.name %>
	  </li>
	  <li>
	  <%= link_to('Logout', signout_path) %>        
	  </li>
	<% else %>
	  <li>
	  <%= link_to('Login', signin_path)  %>  
	  </li>
	<% end %>
	</ul>

<%= yield %>

</body>
</html>
